{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
      "env.d.ts",
      "src/**/*",
      "src/**/*.vue"
    ],
  "compilerOptions": {
    "target": "ES2019",
    "ignoreDeprecations": "5.0",
    "isolatedModules": true,
    "preserveValueImports": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
  },
  "lib": ["es2019"],
  "references": [
    {
      "path": "./tsconfig.vite-config.json"
    }
  ]
}
